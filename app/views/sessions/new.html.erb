<h1> Sign In </h1>

<%= form_with url: session_path do |form| %>

<form>
  <div class="mb-3">
        <%= form.label :email %>
        <br>
        <%= form.text_field :email %>
  </div>

  <div class="mb-3">
        <%= form.label :password %>
        <br>
        <%= form.password_field :password %>
  </div>
  
   
    <%= form.submit "Sign In", class: "btn btn-primary" %>
   

</form>
  


<% end %>
